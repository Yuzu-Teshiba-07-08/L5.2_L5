<h1>商品一覧</h1>
<% @products.each do |e| %>
    <p>商品: <%= e.name %>
        値段: <%= e.price %>円
        <%=link_to '削除', product_path(e), data: { turbo_method: :delete } %>
        <%=link_to 'カート追加', new_cart_item_path(product_id: e.id)%>
    </p>
<% end %>
<%= link_to '商品追加', new_product_path%>
<%= link_to 'カート一覧',carts_show_path %>


